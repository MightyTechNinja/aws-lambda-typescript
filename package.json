{
  "name": "aws-lambda-typescript",
  "version": "1.0.0",
  "description": "An AWS Lambda function in Typescript",
  "license": "GPL-3.0-only",
  "repository": "github:atombrenner/aws-lambda-typescript",
  "scripts": {
    "start": "ts-node -T src/start.ts",
    "test": "jest",
    "clean": "rm -rf ./dist",
    "build": "tsc && webpack",
    "zip": "cd dist && zip -q -r lambda.zip lambda.js",
    "dist": "npm ci && npm dedupe && npm t && npm run build && npm run zip",
    "deploy": "aws lambda update-function-code --function-name MyLambdaFunction --zip-file fileb://dist/lambda.zip",
    "stack": "npm run dist && cd infrastructure && npx cdk deploy"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run dist"
    }
  },
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^1.0.0-gamma.2",
    "@aws-sdk/client-sts": "^1.0.0-gamma.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/aws-lambda": "^8.10.56",
    "@types/jest": "^26.0.0",
    "@types/node": "^12.12.47",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11"
  }
}
