{
  "name": "aws-lambda-typescript",
  "version": "1.0.0",
  "description": "An AWS Lambda function in Typescript",
  "license": "GPL-3.0-only",
  "repository": "github:atombrenner/aws-lambda-typescript",
  "scripts": {
    "test": "jest",
    "clean": "rm -rf ./dist",
    "build": "webpack -p",
    "zip": "npm run clean && npm run build && cd dist && zip -q -r lambda.zip . ",
    "predeploy": "npm run test && npm run zip",
    "deploy": "aws lambda update-function-code --function-name aws-lambda-typescript --zip-file fileb://dist/lambda.zip",
    "analyze": "webpack --profile --json > dist/stats.json && webpack-bundle-analyzer dist/stats.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && pretty-quick --staged"
    }
  },
  "dependencies": {
    "aws-sdk": "^2.411.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.19",
    "@types/jest": "^24.0.6",
    "@types/node": "^11.9.4",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-cli": "^3.2.3"
  }
}
