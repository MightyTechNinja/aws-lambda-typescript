{
  "name": "aws-lambda-typescript",
  "version": "1.0.0",
  "description": "AWS Lambda with Typescript",
  "license": "GPL-3.0-or-later",
  "repository": "github:atombrenner/aws-lambda-typescript",
  "scripts": {
    "housekeeping": "ts-node -T src/housekeeping/start.ts",
    "test": "jest",
    "clean": "rm -rf ./dist",
    "build": "esbuild --bundle --minify --keep-names --sourcemap --sources-content=false --target=node20 --platform=node --outfile=dist/lambda.js src/lambda.ts",
    "zip": "ts-node -T infrastructure/zip.ts",
    "dist": "tsc && npm test && npm run build && npm run zip",
    "deploy": "ts-node -T infrastructure/deploy.ts",
    "prepare": "husky install",
    "stack": "ts-node -T infrastructure/stack.ts"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.460.0",
    "@aws-sdk/client-sts": "^3.460.0"
  },
  "devDependencies": {
    "@atombrenner/cfn-stack": "^2.0.2",
    "@aws-sdk/client-lambda": "^3.460.0",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.10.4",
    "@tsconfig/node20": "^20.1.2",
    "@types/adm-zip": "^0.5.0",
    "@types/aws-lambda": "^8.10.97",
    "@types/jest": "^29.2.3",
    "@types/node": "^20.10.0",
    "adm-zip": "^0.5.9",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "esbuild": "^0.19.8",
    "husky": "^8.0.1",
    "jest": "^29.3.1",
    "prettier": "^3.0.1",
    "ts-node": "^10.7.0",
    "typescript": "^5.2.2"
  }
}
